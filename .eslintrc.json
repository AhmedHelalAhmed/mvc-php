{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "no-console": 0,
    "no-plusplus": 0,
    "func-names": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "no-eval": 0,
    "no-param-reassign": 0,
    "prefer-destructuring": 0
  }
}
