{
  "username" : {
    "table" : "users",
    "user_id_table_name" : "id",
    "filters" : {
      "require" : true,
      "noSpaces" : true,
      "noNumbers": true,
      "noUmlauts": true,
      "noCharExcept" : {
        "chars": "_",
        "times": 2,
        "atFirst": false,
        "atEnd": false,
        "between": true
      },
      "type" : "text",
      "maxLen" : 20,
      "minLen" : 5,
      "unique" : "users"
    }
  },
  "fname" : {
    "table" : "users",
    "user_id_table_name" : "id",
    "filters" : {
      "require" : true,
      "noSpaces" : true,
      "noNumbers": true,
      "noUmlauts": true,
      "noCharExcept" : {},
      "type" : "text",
      "maxLen" : 20,
      "minLen" : 3
    }
  },
  "lname" : {
    "table" : "users",
    "user_id_table_name" : "id",
    "filters" : {
      "require" : true,
      "noSpaces" : true,
      "noNumbers": true,
      "noUmlauts": true,
      "noCharExcept" : {},
      "type" : "text",
      "maxLen" : 20,
      "minLen" : 3
    }
  },
  "gender" : {
    "table" : "users",
    "user_id_table_name" : "id",
    "filters" : {
      "require" : true,
      "containJust" : ["male", "female"]
    }
  },
  "birthday" : {
    "table" : "users",
    "user_id_table_name" : "id",
    "filters" : {
      "require" : true,
      "date" : {
        "format" : "d M Y",
        "start" : "1920",
        "end" : "2004"
      }
    }
  },
  "email" : {
    "table" : "users",
    "user_id_table_name" : "id",
    "filters" : {
      "require" : true,
      "type" : "email",
      "unique" : "users"
    }
  },
  "status" : {
    "table" : "users",
    "user_id_table_name" : "id",
    "filters" : {
      "require" : true,
      "containJust" : [0, 1, 2]
    }
  },
  "country" : {
    "table" : "address",
    "user_id_table_name" : "user_id",
    "filters" : {
      "containJust" : "path:config/icons:[flags]::keys"
    }
  },
  "state" : {
    "table" : "address",
    "user_id_table_name" : "user_id",
    "filters" : {
      "noNumbers": true,
      "noCharExcept" : {},
      "maxLen" : 20,
      "minLen" : 3
    }
  },
  "zip" : {
    "table" : "address",
    "user_id_table_name" : "user_id",
    "filters" : {
      "type" : "number",
      "length" : 5
    }
  },
  "city" : {
    "table" : "address",
    "user_id_table_name" : "user_id",
    "filters" : {
      "noNumbers": true,
      "noCharExcept" : {},
      "type" : "text",
      "maxLen" : 20,
      "minLen" : 3
    }
  },
  "street" : {
    "table" : "address",
    "user_id_table_name" : "user_id",
    "filters" : {
      "noNumbers": true,
      "noCharExcept" : {
        "chars": ".",
        "times": 1,
        "atFirst": false,
        "atEnd": true,
        "between": false
      },
      "type" : "text",
      "maxLen" : 20,
      "minLen" : 3
    }
  },
  "house_number" : {
    "table" : "address",
    "user_id_table_name" : "user_id",
    "filters" : {
      "type" : "number",
      "maxLen" : 5,
      "minLen" : 1
    }
  },
  "additional" : {
    "table" : "address",
    "user_id_table_name" : "user_id",
    "filters" : {
      "type" : "text",
      "maxLen" : 100,
      "minLen" : 5
    }
  }
}
